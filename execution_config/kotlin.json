{
  "Language": "kotlin",
  "Extension": "kt",
  "Description": "kotlinc (kotlin-cli) needs to be installed",
  "Steps": [
    {
      "Runtime": "kotlinc",
      "Command": " {0} -include-Runtime -d {1}.jar",
      "Args": [
        "FILE_PATH",
        "FILE_NAME_WEX"
      ]
    },
    {
      "Runtime": "java",
      "Command": " -jar {0}.jar",
      "Args": [
        "FILE_NAME_WEX"
      ]
    }
  ]
}
