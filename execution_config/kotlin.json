{
  "language": "kotlin",
  "extension": "kt",
  "description": "kotlinc (kotlin-cli) needs to be installed",
  "steps": [
    {
      "runtime": "kotlinc",
      "command": " {0} -include-runtime -d {1}.jar",
      "args": [
        "FILE_PATH",
        "FILE_NAME_WEX"
      ]
    },
    {
      "runtime": "java",
      "command": " -jar {0}.jar",
      "args": [
        "FILE_NAME_WEX"
      ]
    }
  ]
}
